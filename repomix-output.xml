            <div className="grid grid-cols-4 items-center gap-4">
              <Label htmlFor="notes" className="text-right">
                Notes
              </Label>
              <Textarea
                id="notes"
                value={newSessionForm.notes}
                onChange={(e) => setNewSessionForm({ ...newSessionForm, notes: e.target.value })}
