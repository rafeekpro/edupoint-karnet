  try {
    const output = execSync(
      `npx cypress run --spec "cypress/e2e/features/${file}" --reporter json 2>/dev/null`,
      { encoding: 'utf8', stdio: 'pipe' }
    );
    
    const json = JSON.parse(output);
    
    if (json.stats.failures === 0) {
      results.passed.push(file);
