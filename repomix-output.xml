  const [stats] = useState<SessionStats>({
    todaySessions: 3,
    weekSessions: 15,
    completedSessions: 120,
    cancelledSessions: 5,
    totalClients: 25,
    activeClients: 18,
  });

  const todaysSessions = [
    {
      time: '09:00',
