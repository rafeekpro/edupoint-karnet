            <DollarSign className="h-4 w-4 text-muted-foreground" />
          </CardHeader>
          <CardContent>
            <div className="text-2xl font-bold">
              {voucherTypes.length > 0 
                ? formatPrice(voucherTypes.reduce((sum, t) => sum + t.price, 0) / voucherTypes.length)
                : formatPrice(0)}
