              />
              <Route
                path="therapist/clients"
                element={
                  <ProtectedRoute role="therapist">
                    <TherapistClients />
                  </ProtectedRoute>
                }
              />
            </Route>
          </Routes>
      </AuthProvider>
    </Router>
  );
}

export default App;
