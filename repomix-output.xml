                    <TableCell>
                      <p className="text-sm">{type.organization_name || 'Unknown'}</p>
                    </TableCell>
                    <TableCell>
                      <div className="space-y-1 text-sm">
                        <div className="flex items-center gap-1">
                          <Calendar className="h-3 w-3" />
