{
  "stepDefinitions": [
    "cypress/e2e/**/*.ts",
    "cypress/support/step_definitions/**/*.ts"
  ],
  "nonGlobalStepDefinitions": true,
  "filterSpecs": true,
  "omitFiltered": true,
  "json": {
    "enabled": true,
    "output": "cypress/reports/cucumber-report.json"
  },
  "messages": {
    "enabled": true,
    "output": "cypress/reports/messages.ndjson"
  },
  "html": {
    "enabled": true,
    "output": "cypress/reports/cucumber-report.html"
  }
}